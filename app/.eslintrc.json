{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "plugin:vue/recommended",
    "plugin:jest/recommended",
    "@vue/eslint-config-prettier"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "vue/max-attributes-per-line": "off"
  }
}
